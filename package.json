{
  "name": "jsdx-monorepo",
  "private": true,
  "engines": {
    "node": ">=10.0.0",
    "yarn": ">=1.5.0"
  },
  "scripts": {
    "format": "prettier --write 'packages/**/*.{js,md}' './*.{js,md}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "pre-commit": "1.2.2"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "eslint": "^7.6.0",
    "jest": "^26.3.0",
    "postcss-cli": "^7.1.0",
    "prettier": "^2.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^2.23.1",
    "stylelint": "^13.2.0"
  }
}
